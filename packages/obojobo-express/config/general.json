{
	"default": {
		"cookieName": "obo_next",
		"secureCookie": true,
		"bodyParser": {
			"jsonOptions": {
				"limit": "2mb"
			},
			"textOptions": {
				"limit": "2mb"
			},
			"urlencodedOptions": {
				"extended": false
			}
		}
	},
	"development": {
		"cookieSecret": "REPLACE_ME_WITH_YOUR_SECRET"
	},
	"production": {
		"cookieSecret": {"ENV": "OBO_COOKIE_SECRET"}
	}
}
